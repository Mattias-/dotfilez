[user]
	name = Mattias Appelgren
	email = mattias@ppelgren.se
[credential]
	helper = cache --timeout=3600
[alias]
	lg = log --graph --pretty=format:'%Cred%h%C(yellow)%d%Creset %s %Cgreen(%cr) %an %Creset'
    s = status
[core]
    editor = vim
    excludesfile = .gitignore_global
[color]
    branch = auto
    diff = auto
    status = auto
    ui = auto
[push]
    default = simple

