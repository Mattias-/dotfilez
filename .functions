#!/bin/bash

function hashmod() {
    # $1 is lower, $2 is upper, $3 is string to hash
    echo "$(echo "$3" | sum | cut -d ' ' -f 1) % ($2-$1+1) + $1" | bc
}

function dotimes() {
    num=$1
    shift
    RES=()
    for ((i=0; i<$num; i++)) do
        $@
        RES+="$? "
    done
    echo $RES
}

function psgrep() {
    ps -fp $(pgrep -f "$@")
}

function nmaph() {
   nmap -n -sn "$1" -oG - | awk '/Up$/{print $2}'
}
